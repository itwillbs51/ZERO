		<typeAlias type="com.itwillbs.zero.vo.ResponseTokenVO" alias="Token"/>
		<typeAlias type="com.itwillbs.zero.vo.SecondhandVO" alias="Secondhand"/>
		<typeAlias type="com.itwillbs.zero.vo.ChatVO" alias="Chat"/>
		<typeAlias type="com.itwillbs.zero.vo.ChatRoomVO" alias="ChatRoom"/>
